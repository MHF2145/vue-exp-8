{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: '',\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async registerUser() {\n      try {\n        // Mengirim permintaan registrasi ke server\n        const response = await axios.post('http://localhost:3000/register', {\n          username: this.username,\n          email: this.email,\n          password: this.password\n        });\n\n        // Jika berhasil, simpan token (jika diberikan) dan redirect\n        localStorage.setItem('token', response.data.token);\n        alert(response.data.message);\n\n        // Redirect ke halaman dashboard\n        this.$router.push('/dashboard');\n      } catch (error) {\n        // Cek apakah error memiliki response dari server\n        if (error.response && error.response.data && error.response.data.message) {\n          alert(error.response.data.message);\n        } else {\n          // Jika tidak ada response (misal: server mati atau tidak terjangkau)\n          alert('Registration failed. Please check your connection or try again.');\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","email","password","methods","registerUser","response","post","localStorage","setItem","token","alert","message","$router","push","error"],"sources":["/Users/hares/Downloads/vue-exp-8-sementara/client/src/components/UserRegister.vue"],"sourcesContent":["<template>\r\n  <div class=\"flex items-center justify-center min-h-screen bg-gray-100\">\r\n    <div class=\"bg-white p-8 rounded-lg shadow-lg max-w-md w-full\">\r\n      <h2 class=\"text-2xl font-semibold text-center mb-6\">Register</h2>\r\n      <form @submit.prevent=\"registerUser\">\r\n        <div class=\"mb-4\">\r\n          <label for=\"username\" class=\"block text-gray-700\">Username</label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"username\"\r\n            v-model=\"username\"\r\n            class=\"w-full px-4 py-2 border rounded-lg focus:outline-none focus:border-blue-500\"\r\n            required\r\n          />\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label for=\"email\" class=\"block text-gray-700\">Email</label>\r\n          <input\r\n            type=\"email\"\r\n            id=\"email\"\r\n            v-model=\"email\"\r\n            class=\"w-full px-4 py-2 border rounded-lg focus:outline-none focus:border-blue-500\"\r\n            required\r\n          />\r\n        </div>\r\n        <div class=\"mb-6\">\r\n          <label for=\"password\" class=\"block text-gray-700\">Password</label>\r\n          <input\r\n            type=\"password\"\r\n            id=\"password\"\r\n            v-model=\"password\"\r\n            class=\"w-full px-4 py-2 border rounded-lg focus:outline-none focus:border-blue-500\"\r\n            required\r\n          />\r\n        </div>\r\n        <button type=\"submit\" class=\"w-full bg-blue-500 text-white py-2 rounded-lg hover:bg-blue-600\">\r\n          Register\r\n        </button>\r\n      </form>\r\n      \r\n      <p class=\"mt-4 text-center text-gray-600\">\r\n        Already have an account? \r\n        <router-link to=\"/login\" class=\"text-blue-500 hover:text-blue-700\">Login Here</router-link>\r\n      </p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: '',\r\n      email: '',\r\n      password: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async registerUser() {\r\n      try {\r\n        // Mengirim permintaan registrasi ke server\r\n        const response = await axios.post('http://localhost:3000/register', {\r\n          username: this.username,\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n        \r\n        // Jika berhasil, simpan token (jika diberikan) dan redirect\r\n        localStorage.setItem('token', response.data.token);\r\n        alert(response.data.message);\r\n        \r\n        // Redirect ke halaman dashboard\r\n        this.$router.push('/dashboard');\r\n      } catch (error) {\r\n        // Cek apakah error memiliki response dari server\r\n        if (error.response && error.response.data && error.response.data.message) {\r\n          alert(error.response.data.message);\r\n        } else {\r\n          // Jika tidak ada response (misal: server mati atau tidak terjangkau)\r\n          alert('Registration failed. Please check your connection or try again.');\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add any scoped styling here if needed */\r\n</style>\r\n"],"mappings":";AAkDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF;QACA,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,gCAAgC,EAAE;UAClEN,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;;QAEF;QACAK,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACN,IAAI,CAACU,KAAK,CAAC;QAClDC,KAAK,CAACL,QAAQ,CAACN,IAAI,CAACY,OAAO,CAAC;;QAE5B;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;MACjC,EAAE,OAAOC,KAAK,EAAE;QACd;QACA,IAAIA,KAAK,CAACT,QAAO,IAAKS,KAAK,CAACT,QAAQ,CAACN,IAAG,IAAKe,KAAK,CAACT,QAAQ,CAACN,IAAI,CAACY,OAAO,EAAE;UACxED,KAAK,CAACI,KAAK,CAACT,QAAQ,CAACN,IAAI,CAACY,OAAO,CAAC;QACpC,OAAO;UACL;UACAD,KAAK,CAAC,iEAAiE,CAAC;QAC1E;MACF;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}