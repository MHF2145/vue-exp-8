{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// Import PopUpGuest instead of PopUp\nimport PopUpGuest from \"./PopUpGuest.vue\";\nexport default {\n  components: {\n    PopUpGuest\n  },\n  data() {\n    return {\n      pokemonList: [],\n      selectedGeneration: null,\n      currentPage: 1,\n      itemsPerPage: 24,\n      selectedPokemon: null,\n      favorites: [] // Track favorited Pokémon\n    };\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.pokemonList.length / this.itemsPerPage);\n    },\n    paginatedPokemon() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.pokemonList.slice(start, end);\n    }\n  },\n  methods: {\n    async fetchPokemon(gen) {\n      this.pokemonList = [];\n      this.currentPage = 1;\n      const genRanges = [{\n        gen: 1,\n        range: [1, 151]\n      }, {\n        gen: 2,\n        range: [152, 251]\n      }, {\n        gen: 3,\n        range: [252, 386]\n      }, {\n        gen: 4,\n        range: [387, 494]\n      }, {\n        gen: 5,\n        range: [495, 649]\n      }, {\n        gen: 6,\n        range: [650, 722]\n      }, {\n        gen: 7,\n        range: [723, 809]\n      }, {\n        gen: 8,\n        range: [810, 906]\n      }, {\n        gen: 9,\n        range: [907, 1025]\n      }];\n      const range = genRanges.find(g => g.gen === gen)?.range;\n      if (range) {\n        for (let id = range[0]; id <= range[1]; id++) {\n          await this.fetchAndStorePokemon(id);\n        }\n      }\n    },\n    async fetchAndStorePokemon(id) {\n      try {\n        const response = await fetch(`https://pokeapi.co/api/v2/pokemon/${id}`);\n        const data = await response.json();\n        this.pokemonList.push({\n          id: data.id,\n          name: data.name,\n          height: data.height / 10,\n          // Height in meters\n          types: data.types,\n          abilities: data.abilities,\n          sprite: data.sprites.front_default\n        });\n      } catch (error) {\n        console.error(`Error fetching data for Pokémon ID ${id}:`, error);\n      }\n    },\n    showPopup(pokemon) {\n      this.selectedPokemon = pokemon;\n    },\n    markFavorite(pokemon) {\n      if (!this.favorites.find(fav => fav.id === pokemon.id)) {\n        this.favorites.push(pokemon);\n        console.log(`${pokemon.name} added to favorites.`);\n      }\n    },\n    switchPokemon(direction) {\n      const currentIndex = this.pokemonList.findIndex(p => p.id === this.selectedPokemon.id);\n      if (direction === 'next' && currentIndex < this.pokemonList.length - 1) {\n        this.selectedPokemon = this.pokemonList[currentIndex + 1];\n      } else if (direction === 'previous' && currentIndex > 0) {\n        // Update this line\n        this.selectedPokemon = this.pokemonList[currentIndex - 1];\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["PopUpGuest","components","data","pokemonList","selectedGeneration","currentPage","itemsPerPage","selectedPokemon","favorites","computed","totalPages","Math","ceil","length","paginatedPokemon","start","end","slice","methods","fetchPokemon","gen","genRanges","range","find","g","id","fetchAndStorePokemon","response","fetch","json","push","name","height","types","abilities","sprite","sprites","front_default","error","console","showPopup","pokemon","markFavorite","fav","log","switchPokemon","direction","currentIndex","findIndex","p","nextPage","prevPage"],"sources":["/Users/hares/Downloads/vue-exp-8-sementara/client/src/components/GenButtonGuest.vue"],"sourcesContent":["<template>\n    <div class=\"flex flex-col items-center\">\n        <PopUpGuest v-if=\"selectedPokemon\" :pokemon=\"selectedPokemon\" @close=\"selectedPokemon = null\"\n            @favorite=\"markFavorite\" @switch=\"switchPokemon\" />\n\n        <div class=\"mt-4\">\n            <select v-model=\"selectedGeneration\" @change=\"fetchPokemon(selectedGeneration)\"\n                class=\"px-4 py-2 bg-blue-500 text-white rounded hover:bg-blue-700 transition duration-200\">\n                <option disabled value=\"\">Select Generation</option>\n                <option v-for=\"i in 9\" :key=\"i\" :value=\"i\">\n                    Gen {{ i }}\n                </option>\n            </select>\n        </div>\n\n        <div v-if=\"selectedGeneration\"\n            class=\"grid grid-cols-3 sm:grid-cols-4 md:grid-cols-5 lg:grid-cols-6 gap-4 mt-6 w-full px-4\">\n            <button v-for=\"pokemon in paginatedPokemon\" :key=\"pokemon.id\"\n                class=\"bg-white rounded shadow p-2 flex flex-col items-center justify-center\"\n                @click=\"showPopup(pokemon)\">\n                <img :src=\"pokemon.sprite\" :alt=\"pokemon.name\" class=\"w-full h-auto object-contain mb-2\" />\n                <h2 class=\"text-lg font-semibold truncate\">{{ pokemon.name }}</h2>\n            </button>\n        </div>\n\n        <div v-if=\"selectedGeneration\" class=\"flex space-x-4 mt-4\">\n            <button v-if=\"currentPage > 1\" @click=\"prevPage\"\n                class=\"px-4 py-2 bg-blue-500 text-white rounded hover:bg-blue-700 transition duration-200\">\n                Previous\n            </button>\n            <button v-if=\"currentPage < totalPages\" @click=\"nextPage\"\n                class=\"px-4 py-2 bg-blue-500 text-white rounded hover:bg-blue-700 transition duration-200\">\n                Next\n            </button>\n        </div>\n    </div>\n</template>\n\n<script>\n// Import PopUpGuest instead of PopUp\nimport PopUpGuest from \"./PopUpGuest.vue\";\n\nexport default {\n    components: { PopUpGuest },\n    data() {\n        return {\n            pokemonList: [],\n            selectedGeneration: null,\n            currentPage: 1,\n            itemsPerPage: 24,\n            selectedPokemon: null,\n            favorites: [], // Track favorited Pokémon\n        };\n    },\n    computed: {\n        totalPages() {\n            return Math.ceil(this.pokemonList.length / this.itemsPerPage);\n        },\n        paginatedPokemon() {\n            const start = (this.currentPage - 1) * this.itemsPerPage;\n            const end = start + this.itemsPerPage;\n            return this.pokemonList.slice(start, end);\n        },\n    },\n    methods: {\n        async fetchPokemon(gen) {\n            this.pokemonList = [];\n            this.currentPage = 1;\n            const genRanges = [\n                { gen: 1, range: [1, 151] },\n                { gen: 2, range: [152, 251] },\n                { gen: 3, range: [252, 386] },\n                { gen: 4, range: [387, 494] },\n                { gen: 5, range: [495, 649] },\n                { gen: 6, range: [650, 722] },\n                { gen: 7, range: [723, 809] },\n                { gen: 8, range: [810, 906] },\n                { gen: 9, range: [907, 1025] },\n            ];\n            const range = genRanges.find((g) => g.gen === gen)?.range;\n            if (range) {\n                for (let id = range[0]; id <= range[1]; id++) {\n                    await this.fetchAndStorePokemon(id);\n                }\n            }\n        },\n        async fetchAndStorePokemon(id) {\n            try {\n                const response = await fetch(`https://pokeapi.co/api/v2/pokemon/${id}`);\n                const data = await response.json();\n                this.pokemonList.push({\n                    id: data.id,\n                    name: data.name,\n                    height: data.height / 10, // Height in meters\n                    types: data.types,\n                    abilities: data.abilities,\n                    sprite: data.sprites.front_default,\n                });\n            } catch (error) {\n                console.error(`Error fetching data for Pokémon ID ${id}:`, error);\n            }\n        },\n        showPopup(pokemon) {\n            this.selectedPokemon = pokemon;\n        },\n        markFavorite(pokemon) {\n            if (!this.favorites.find((fav) => fav.id === pokemon.id)) {\n                this.favorites.push(pokemon);\n                console.log(`${pokemon.name} added to favorites.`);\n            }\n        },\n        switchPokemon(direction) {\n            const currentIndex = this.pokemonList.findIndex(p => p.id === this.selectedPokemon.id);\n            if (direction === 'next' && currentIndex < this.pokemonList.length - 1) {\n                this.selectedPokemon = this.pokemonList[currentIndex + 1];\n            } else if (direction === 'previous' && currentIndex > 0) { // Update this line\n                this.selectedPokemon = this.pokemonList[currentIndex - 1];\n            }\n        },\n        nextPage() {\n            if (this.currentPage < this.totalPages) {\n                this.currentPage++;\n            }\n        },\n        prevPage() {\n            if (this.currentPage > 1) {\n                this.currentPage--;\n            }\n        },\n    },\n};\n</script>\n\n<style scoped>\nimg {\n    max-height: 150px;\n}\n</style>"],"mappings":";AAuCA;AACA,OAAOA,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACXC,UAAU,EAAE;IAAED;EAAW,CAAC;EAC1BE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE,EAAE;MACfC,kBAAkB,EAAE,IAAI;MACxBC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,EAAE;MAChBC,eAAe,EAAE,IAAI;MACrBC,SAAS,EAAE,EAAE,CAAE;IACnB,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,UAAUA,CAAA,EAAG;MACT,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACT,WAAW,CAACU,MAAK,GAAI,IAAI,CAACP,YAAY,CAAC;IACjE,CAAC;IACDQ,gBAAgBA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACV,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACxD,MAAMU,GAAE,GAAID,KAAI,GAAI,IAAI,CAACT,YAAY;MACrC,OAAO,IAAI,CAACH,WAAW,CAACc,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IAC7C;EACJ,CAAC;EACDE,OAAO,EAAE;IACL,MAAMC,YAAYA,CAACC,GAAG,EAAE;MACpB,IAAI,CAACjB,WAAU,GAAI,EAAE;MACrB,IAAI,